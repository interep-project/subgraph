{
    "name": "subgraph",
    "version": "0.3.0",
    "description": "InterRep subgraph definition (The Graph).",
    "main": "index.js",
    "repository": "git@github.com:InterRep/subgraph.git",
    "license": "MIT",
    "scripts": {
        "codegen": "graph codegen",
        "build": "graph build",
        "deploy": "graph deploy --product hosted-service interrep/interrep-groups-kovan",
        "create-local": "graph create --node http://localhost:8020/ interrep/InterRep-Groups-kovan",
        "remove-local": "graph remove --node http://localhost:8020/ interrep/InterRep-Groups-kovan",
        "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 interrep/InterRep-Groups-kovan",
        "lint": "eslint src --ext js,jsx,ts,tsx",
        "commit": "cz",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "@graphprotocol/graph-ts": "^0.22.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@graphprotocol/graph-cli": "^0.22.3",
        "@types/node": "^16.6.1",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^1.7.0",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2",
        "ts-node": "^10.2.0",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
